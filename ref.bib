@article{Zucco2009,
  abstract = {In this paper I use data from Brazil to show that the comparison
    between ideology—measured from survey data — and behavior — measured from
      roll call data — supports the claim that the president plays an important
      role in influencing the behavior of legislators. Moreover, statistical
      analysis suggests that this influence is channeled through the
      distribution of pork and nominations to cabinet positions. I find that
      ideology does not fully explain the patterns of legislative behavior
      observed since the return to democracy, and that its influence on
      behavior has declined over time.},
  author = {Zucco, Cesar},
  doi = {10.1017/S0022381609090896},
  issn = {0022-3816},
  journal = {The Journal of Politics},
  number = {03},
  pages = {1076},
  title = {{Ideology or What? Legislative Behavior in Multiparty Presidential
    Settings}},
  url = {http://www.jstor.org/stable/10.1017/S0022381609090896},
  volume = {71},
  year = {2009}
}

